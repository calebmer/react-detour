{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "target": "es3",
    "moduleResolution": "node",
    "sourceMap": true,
    "declaration": true,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictNullChecks": true
  },
  "exclude": [
    "node_modules"
  ]
}
